#include <iostream>
#include <algorithm>
#include <cstring>
using namespace std;

int main() {
    char nameOfStudents[10][50];
    int n;

    cout << "Enter number of students: ";
    cin >> n;

    if (n > 10) {
        cout << "You can enter at most 10 names." << endl;
        return 0;
    }

    cout << "Enter " << n << " names:" << endl;
    for (int i = 0; i < n; i++) {
        cin >> nameOfStudents[i];   
    }

    for (int i = 0; i < n - 1; i++) {
        for (int j = i + 1; j < n; j++) {
            if (strcmp(nameOfStudents[i], nameOfStudents[j]) > 0) {
                char temp[50];
                strcpy(temp, nameOfStudents[i]);
                strcpy(nameOfStudents[i], nameOfStudents[j]);
                strcpy(nameOfStudents[j], temp);
            }
        }
    }

    cout << "\nSorted Names:" << endl;
    for (int i = 0; i < n; i++) {
        cout << nameOfStudents[i] << endl;
    }

    return 0;
}
